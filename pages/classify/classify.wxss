page{  
  background: #f5f5f5;  
} 
.container{
  position: relative;
  width: 100%;
  height: 100%;
  color: #939393;
  background-color: #fff;
}
.nav_left{
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
}
.nav_left_items{
  height: 40px;
  line-height: 40px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  background: #fff;  
  color: #f0145a; 
}
.nav_right{
  position: absolute;
  top: 0;
  right:0;
  flex: 1;
  width: 75%;
  height: 10000px;
  padding: 10px;
  box-sizing: border-box;
  background: #fff;
}
.nav_right_items{
  float: left;
  width: 33.33%;
  height: 120px;
  text-align: center;
}
.nav_right_items image{
  width: 60px;
  height: 60px;
  margin-top: 15px;
}
.nav_right_items text{
  display: block;
  margin-top: 15px;
  font-size: 14px;
  color: black;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}