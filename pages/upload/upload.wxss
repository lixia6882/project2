.container{
  background: #fff;
}
.upload{
  width: 260rpx;
  height:260rpx;
  border:1px solid #ccc;
  margin:0 auto;
  line-height: 260rpx;
  text-align: center;
  font-size: 30rpx;
  margin-top: 40rpx;
  position: relative;
}
.upload image{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.look{
  width:85%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fea500;
  color: #fff;
  margin:0 auto;
  margin-top: 60rpx;
  text-align: center;
  border-radius: 15rpx;
}
.uploading{
  width:85%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fea500;
  color: #fff;
  margin:0 auto;
  margin-top: 30rpx;
  text-align: center;
  border-radius: 15rpx;
}
