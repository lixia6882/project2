/* pages/do_load/do_load.wxss */
.container {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  overflow: auto;
  background: #f0f0f0;
  position: relative;
}
.main{
  width: 86%;
  padding: 0 20rpx;
  background: #fff;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 20rpx;
  font-size: 34rpx;
  
}
.up{
  display: block;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  border-bottom: 1px solid #ccc;
}
.upload{
  width: 228rpx;
  margin: 30rpx;
  text-align: center;
  color: #b6b6b6;
}
.images1{
  width: 228rpx;
  height: 148rpx;
}
.btn{
  width: 86%;
  margin: 0 auto;
  margin-top: 150rpx;
  background: #f23861;
  color: #fff;
  line-height:100rpx;
  border-radius: 20rpx;
  text-align: center;
}
.promp{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top: 1px dashed #ccc;
  font-size: 34rpx;
}
.tex1{
  color: #f76069;
  display: block;
  line-height: 100rpx;
  padding-left: 40rpx;
}

.tex2{
  color: #b9b9b9;
  display: block;
  font-size: 30rpx;
  line-height: 45rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  margin-bottom: 20rpx;
}
.preview{
width: 100%;
height: 100%;
position:absolute;
top: 0;
left: 0;
z-index: 1000;
background: #000;
padding: 100rpx 0;
}
.preview image{
  width: 100%;
  height: 100%;
}
.back{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1000;
  background:#000;
  background: rgba(0,0,0,0.5);
}
.box{
  width: 80%;
  height: 240rpx;
  margin: 0 auto;
  position: absolute;
  right: 0;
  left: 0;
  top:50%;
  bottom: 0;
  margin-top:-120rpx;

}
.box view{
  background:#fff;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-bottom: 1px solid #ccc;
}
.back view:last-child{
  border-bottom: none;
}


