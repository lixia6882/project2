.container {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #f0f0f0;
  position: relative;
  overflow: auto;
}
.main{
  width: 86%;
  padding: 0 20rpx;
  background: #fff;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 20rpx;
  font-size: 34rpx;
  
}
.upphoto{
  width: 100%;
  display: -webkit-flex;
}
.up{
  display: block;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  border-bottom: 1px solid #ccc;
}
.upload{
  width: 228rpx;
  margin: 30rpx;
  text-align: center;
  color: #b6b6b6;
}
.uploads{
  width: 250rpx;
}
.images1{
  width: 228rpx;
  height: 148rpx;
}
.btn{
  width: 86%;
  margin: 0 auto;
  margin-top: 150rpx;
  background: #f23861;
  color: #fff;
  line-height:100rpx;
  border-radius: 20rpx;
  text-align: center;
}
.promp{
  width: 100%;
  border-top: 1px dashed #ccc;
  font-size: 34rpx;
  margin-top:78rpx;
}
.tex1{
  color: #f76069;
  display: block;
